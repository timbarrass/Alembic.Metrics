<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="processCountingSource" type="Sources.ProcessCountingSourceConfiguration, Sources" />
    <section name="performanceCounterSource" type="Sources.PerformanceCounterDataSourceConfiguration, Sources" />
  </configSections>

  <appSettings>
    <add key="outputPath" value="." />
  </appSettings>

  <performanceCounterSource>
    <counters>
      <add categoryName="Memory" counterName="Committed Bytes" />
      <add categoryName="Processor" counterName="% Processor Time" instanceName="_Total" />
    </counters>
  </performanceCounterSource>
  
  <processCountingSource>
    <processes>
      <add name="Google Chrome" exe="chrome" />
      <add name="Microsoft Excel" exe="excel" />
    </processes>
  </processCountingSource>
</configuration>